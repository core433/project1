<% provide(:title, 'New Project') %>
<h1>New Project</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@project) do |f| %>
    	<%= render 'shared/error_messages', object: f.object %>

      	<%= f.label :title %>
      	<%= f.text_field :title %>

      	<center>
      		<br/>
      		<%= f.submit "Create project", class: "btn btn-large btn-primary" %>
      	</center>
    <% end %>
  </div>
</div>